<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-schema="hibernate" match-name=".*"
		exclude="true">
	</table-filter>
	<table-filter match-schema="mydb" match-name=".*" exclude="true"></table-filter>
	<table-filter match-schema="sys" match-name=".*" exclude="true"></table-filter>
</hibernate-reverse-engineering>